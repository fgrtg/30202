  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "import turtle as t\n",
    "n = 60    \n",
    "t.shape('turtle')\n",
    "t.speed('fastest')\n",
    "t.color('blue','red')\n",
    "t.begin_fill()\n",
    "for i in range(n):\n",
    "    t.circle(120)   \n",
    "    t.right(360 / n)\n",
    "t.end_fill()\n",
    "t.done()"
   ]
